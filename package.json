{
  "name": "FunRunning",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:staging": "react-native run-android --variant=releaseStaging",
    "android:release": "react-native run-android --variant=release",
    "ios": "arch -x86_64 react-native run-ios",
    "ios:staging": "arch -x86_64 react-native run-ios --configuration Staging",
    "ios:release": "arch -x86_64 react-native run-ios --configuration Release",
    "ios:device": "react-native run-ios --device",
    "pod": "cd ios && arch -x86_64 pod install && cd ..",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "start": "react-native start",
    "rstart": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint .",
    "type-check": "tsc",
    "build:tailwind": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn",
    "dev:tailwind": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\"",
    "preapk:debug": "sed -i '' 's/const isStaging = false/const isStaging = true/' ./babel.config.js",
    "apk:debug": "cd ./android && ./gradlew app:assembleDebug && ./gradlew installDebug",
    "preapk:staging": "sed -i '' 's/const isStaging = false/const isStaging = true/' ./babel.config.js",
    "apk:staging": "cd ./android && ./gradlew app:assembleReleaseStaging",
    "preapk:release": "sed -i '' 's/const isStaging = true/const isStaging = false/' ./babel.config.js",
    "apk:release": "cd ./android && ./gradlew app:assembleRelease",
    "beta:ios": "cd ./ios && bundle exec fastlane beta version:test",
    "beta:android": "cd ./android && bundle exec fastlane beta version:patch",
    "beta": "yarn beta:android && yarn beta:ios",
    "release:ios": "cd ./ios && bundle exec fastlane release version:patch",
    "release:android": "cd ./android && bundle exec fastlane release version:patch",
    "release": "yarn release:android && yarn release:ios"
  },
  "dependencies": {
    "@dongminyu/react-native-step-counter": "^0.2.3",
    "@ovalmoney/react-native-fitness": "^0.5.3",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-native": "0.72.7",
    "react-native-health": "^1.18.0",
    "react-native-permissions": "^4.0.0",
    "tailwind-rn": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "postcss": "^8.4.31",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "^3.3.5",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
